<h1><%= @artist.name %></h1>
<table>
    <tr>
        <th>Songs</th>
        <th>Genre Of Song</th>
    </tr>
    <% @artist.songs.each do |song| %>
    <tr>
        <td>
            <a href='/songs/<%= song.slug %>'><%= song.name %></a>
        </td>
        <td>
            <% song.genres.each do |genre| %>
                <a href='/genres/<%= genre.slug %>'><%= genre.name %></a>
            <% end %>
        </td>
    </tr>
    <% end %>
</table>
<p/>
<a href='/artists'><input type='button' value='Return to Artist List'></a>