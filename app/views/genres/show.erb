<h1><%= @genre.name %></h1>
<table>
    <tr>
        <th>Songs</th>
        <th>Artist Of Song</th>
    </tr>
    <% @genre.songs.each do |song| %>
    <tr>
        <td>
            <a href='/songs/<%= song.slug %>'><%= song.name %></a>
        </td>
        <td>
                <a href='/artists/<%= song.artist.slug %>'><%= song.artist.name %></a>
        </td>
    </tr>
    <% end %>
</table>
<p/>
<a href='/genres'><input type='button' value='Return to Genre List'></a>